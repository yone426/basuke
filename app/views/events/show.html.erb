<div class="new">
  <p id="notice"><%= notice %></p>

  <p>
    <strong>タイトル：</strong>
    <%= @event.title %>
  </p>

  <p>
    <strong>内容：</strong>
    <%= @event.body %>
  </p>

  <p>
    <strong>開始時間：</strong>
    <%= @event.start_date %>
  </p>

  <p>
    <strong>終了時間：</strong>
    <%= @event.end_date %>
  </p>

  <%= link_to '編集', edit_event_path(@event),class: "delete" %> 
  <%= link_to '削除', "/events/#{@event.id}", method: :delete,class: "delete" %>
  <%# <%= link_to 'Back', events_path %>
</div>